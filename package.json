    "test:watch": "npm test -- --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:light": "STORYBOOK_THEME=light storybook dev -p 6006",
    "storybook:dark": "STORYBOOK_THEME=dark storybook dev -p 6006"
